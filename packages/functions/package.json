{
  "name": "@mori-app/functions",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "watch": "tsup --watch src --config tsup.config.ts"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
